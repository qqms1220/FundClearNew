@model IEnumerable<FundClear.Models.到期项目表>

@{
    ViewBag.Title = "到期项目表";
}

<h2>到期项目表</h2>
@Html.ActionLink("导出到Excel", "ExportToExcel", null, new { @class = "btn btn-primary btn-sm" })
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.融资方)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.合伙企业)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.还款日期)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.投资金额))(万元)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.回款本金)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.收益金额)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.服务费金额)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.回款金额)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.打款日期)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.产品期限)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.兑付方式)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.本期兑付内容)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.融资方)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.合伙企业)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.还款日期)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.投资金额)万
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.回款本金)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.收益金额)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.服务费金额)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.回款金额)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.打款日期)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.产品期限)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.兑付方式)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.本期兑付内容)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.ID }) |
            @Html.ActionLink("详情", "Details", new { id=item.ID }) 
            @*@Html.ActionLink("Delete", "Delete", new { id=item.ID })*@
        </td>
    </tr>
}

</table>

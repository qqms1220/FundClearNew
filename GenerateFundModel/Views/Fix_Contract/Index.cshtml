@model IEnumerable<FundClear.Models.Fix_Contract>

@{
    ViewBag.Title = "Index";
}

<h2>固收类合同列表</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Contract_Status.状态)
        </th>    
        <th>
            @Html.DisplayNameFor(model => model.投资人姓名)
        </th>  
        <th>
            @Html.DisplayNameFor(model => model.金额)
        </th>      
        <th>
            @Html.DisplayNameFor(model => model.Fix_Product.产品名称)
        </th>       
        <th>
            @Html.DisplayNameFor(model => model.投资人银行账户名)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.投资人开户银行)
        </th> 
        <th>
            @Html.DisplayNameFor(model => model.投资人银行账号)
        </th>              
        <th>
            @Html.DisplayNameFor(model => model.购买日期)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.成立日期)
        </th>        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Contract_Status.状态)
        </td>      
        <td>
            @Html.DisplayFor(modelItem => item.投资人姓名)
        </td> 
        <td>
            @Html.DisplayFor(modelItem => item.金额)
        </td>    
        <td>
            @Html.DisplayFor(modelItem => item.Fix_Product.产品名称)
        </td>            
        <td>
            @Html.DisplayFor(modelItem => item.投资人银行账户名)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.投资人开户银行)
        </td>    
        <td>
            @Html.DisplayFor(modelItem => item.投资人银行账号)
        </td>          
        <td>
            @Html.DisplayFor(modelItem => item.购买日期)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.成立日期)
        </td>        
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.Contract_id }) |
            @Html.ActionLink("详情", "Details", new { id=item.Contract_id }) |
            @Html.ActionLink("删除", "Delete", new { id=item.Contract_id })
        </td>
    </tr>
}

</table>

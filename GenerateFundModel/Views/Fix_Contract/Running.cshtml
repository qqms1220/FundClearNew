@model IEnumerable<FundClear.Models.Fix_running_tab>

@{
    ViewBag.Title = "固收基金流水账";
}

<h2>固收基金流水账</h2>
<table class="table">
    <tr>
        <th>
            合同流水标识号
        </th>
        <th>
            合同标识号
        </th>
        <th>
            @Html.DisplayNameFor(model => model.金额)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.发生时间)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.合同流水摘要)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fix_running_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contract_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.金额)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.发生时间)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.合同流水摘要)
            </td>           
        </tr>
    }
</table>
<div>
    @Html.ActionLink("返回列表", "Index")
</div>


